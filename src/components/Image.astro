---
export interface Props {
  src: string;
  alt: string;
  height: number;
  width: number;
  loading?: "lazy" | "eager";
  class?: string;
}

const { src, alt, height, width, loading = "lazy", class: className = "" } = Astro.props;
---

<img {src} {alt} {height} {width} {loading} decoding="async" class={className} />
